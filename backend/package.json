  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^ @libs/models(|/.*)$": "<rootDir>/libs/models/src/$1",
      "^@libs/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@libs/dtos(|/.*)$": "<rootDir>/libs/dtos/src/$1",
      "^@libs/enums(|/.*)$": "<rootDir>/libs/enums/src/$1",
      "^@libs/logger(|/.*)$": "<rootDir>/libs/logger/src/$1",
      "^@libs/s3(|/.*)$": "<rootDir>/libs/s3/src/$1",
      "^@libs/types(|/.*)$": "<rootDir>/libs/types/src/$1"
