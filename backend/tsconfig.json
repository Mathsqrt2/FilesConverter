{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "types": [
      "node",
      "jest",
      "./.env.d.ts"
    ],
    "paths": {
      "@libs/database": [
        "libs/database/src"
      ],
      "@libs/database/*": [
        "libs/database/src/*"
      ],
      "@libs/dtos": [
        "libs/dtos/src"
      ],
      "@libs/dtos/*": [
        "libs/dtos/src/*"
      ],
      "@libs/enums": [
        "libs/enums/src"
      ],
      "@libs/enums/*": [
        "libs/enums/src/*"
      ],
      "@libs/logger": [
        "libs/logger/src"
      ],
      "@libs/logger/*": [
        "libs/logger/src/*"
      ],
      "@libs/s3": [
        "libs/s3/src"
      ],
      "@libs/s3/*": [
        "libs/s3/src/*"
      ],
      "@libs/types": [
        "libs/types/src"
      ],
      "@libs/types/*": [
        "libs/types/src/*"
      ]
    }
  }
}